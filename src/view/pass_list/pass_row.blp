using Gtk 4.0;
using Adw 1;

template PassRow : Gtk.ListBoxRow
{
    can-focus: true;
    margin-top: 6;
    selectable: true;

    Gtk.Box box
    {
        can-focus: false;
        margin-bottom: 0;
        margin-end: 0;
        margin-start: 0;
        margin-top: 0;
        spacing: 12;

        .PassIcon icon {}

        Gtk.Label name
        {
            hexpand: true;
            wrap: true;
            xalign: 0;
        }
    }
}

